{
  "swagger": {
    "entryFile": "./src/routes/routes.ts",
    "outputDirectory": "./src/swagger",
    "specMerging": "recursive",
    "spec": {},
    "securityDefinitions": {
      "jwt": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "routes": {
    "entryFile": "./src/controllers/index.ts",
    "routesDir": "./src/routes",
    "basePath": "/service/",
    "authenticationModule": "./src/authentication/authentication.ts",
    "middleware": "express",
    "iocModule": "./src/config/ioc"
  }
}